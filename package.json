{
    "name": "abs",
    "displayName": "abs",
    "description": "Support for the Abs language",
    "version": "0.0.2",
    "publisher": "abs",
    "repository": {
        "type": "git",
        "url": "https://github.com/abstools/abs-vs-code.git"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "scripts": {
        "package": "vsce package"
    },
    "contributes": {
        "languages": [
            {
                "id": "abs",
                "aliases": [
                    "Abs",
                    "abs"
                ],
                "extensions": [
                    ".abs"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "abs",
                "scopeName": "source.abs",
                "path": "./syntaxes/abs.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "vsce": "^1.83.0"
    }
}
